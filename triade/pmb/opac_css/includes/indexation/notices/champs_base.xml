	<!-- Infos d'exemplaires -->
	<field name="search_exemplaire" external="yes" id="090" datatype="expl">
		<!-- Indexation de l'article avec les exemplaires du bulletin auquel il est relié -->
		<table name="exemplaires">
			<filter>(notices.niveau_biblio='a' and notices.niveau_hierar=2)</filter>
			<tablefield name="extexpl_codebar" id="01" pond="100">expl_cb</tablefield>
			<tablefield name="extexpl_cote" id="02" pond="100">expl_cote</tablefield>
			<tablefield name="extexpl_section" id="03" pond="100">section_libelle</tablefield>
			<tablefield name="extexpl_location" id="04" pond="100">location_libelle</tablefield>
			<tablefield name="extexpl_statut" id="05" pond="100">statut_libelle</tablefield>
			<tablefield name="extexpl_doctype" id="06" pond="100">tdoc_libelle</tablefield>
			<tablefield name="empr_tpl_stat" id="07" pond="100">codestat_libelle</tablefield>
			<tablefield name="proprietary_search" id="08" pond="100">lender_libelle</tablefield>
			<!-- A activer dans le fichier subst si l'on veut indexer l'exemplaire avec les sur-localisations -->
			<!-- <tablefield name="expl_header_surloc_libelle" id="09" pond="100">surloc_libelle</tablefield> -->
			<tablekey>expl_id</tablekey>
			<link type="n1">
					<table>analysis</table>
					<tablekey>analysis_notice</tablekey>
					<externaltable>notices</externaltable>
					<externalfield>notice_id</externalfield>
					<!-- <linkrestrict> AND (notices.niveau_biblio='a' AND notices.niveau_hierar=2)</linkrestrict> -->
				</link>
			<link type="n1">
					<table>bulletins</table>
					<tablekey>bulletin_id</tablekey>
					<externaltable>analysis</externaltable>
					<externalfield>analysis_bulletin</externalfield>
			</link>
			<link type="n1">
					<table>exemplaires</table>
					<tablekey>expl_bulletin</tablekey>
					<externaltable>bulletins</externaltable>
					<externalfield>bulletin_id</externalfield>
					<linkrestrict>AND exemplaires.expl_notice=0</linkrestrict>
			</link>
			<link type="n1">
				<table>docs_section</table>
				<tablekey>idsection</tablekey>
				<externalfield>expl_section</externalfield>
				<linkrestrict>AND docs_section.section_visible_opac=1</linkrestrict>
			</link>
			<link type="n1">
				<table>docs_location</table>
				<tablekey>idlocation</tablekey>
				<externalfield>expl_location</externalfield>
				<linkrestrict>AND docs_location.location_visible_opac=1</linkrestrict>
			</link>
			<link type="n1">
				<table>docs_statut</table>
				<tablekey>idstatut</tablekey>
				<externalfield>expl_statut</externalfield>
				<linkrestrict>AND docs_statut.statut_visible_opac=1</linkrestrict>
			</link>
			<link type="n1">
				<table>docs_type</table>
				<tablekey>idtyp_doc</tablekey>
