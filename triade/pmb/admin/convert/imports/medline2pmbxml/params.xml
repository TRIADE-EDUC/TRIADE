<params>
    <!-- Nom de l'import -->
    <importname>MEDLINE -&gt; PMBXML</importname>
    <!-- Nombre de notices traitées par passe -->
    <nperpass>200</nperpass>
    <!-- Type de l'entrée -->
    <input type="custom" script="medline2pmbxml_input.class.php"/>
    <!-- type de la sortie -->
    <output importable="no" type="xml" suffix="xml">
		<rootelement>pmbmarc</rootelement>
	</output>

    <step type="custom">
        <!-- Conversion Endnote en pmbxml -->
        <script>medline2pmbxml.class.php</script>
		<callback>convert_data</callback>
    </step>
</params>