$Id: params.xml,v 1.2 2018-07-25 06:19:18 dgoron Exp $ -->

<params>
    <!-- Nom de l'import -->
    <importname>delphe -&gt; PMBXML</importname>
    <!-- Nombre de notices traitées par passe -->
    <nperpass>200</nperpass>
    <!-- Type de l'entrée -->
    <input type="custom" script="delphe2unimarciso_input.class.php"/>
    <!-- type de la sortie -->
     <output importable="yes" type="iso_2709" suffix="uni">
        <rootelement>unimarc</rootelement>
    </output>

    <step type="custom">
        <!-- Conversion RIS en pmbxml -->
        <script>deplhe2pmbxml.inc.php</script>
		<callback>convert_delphe</callback>
    </step>
    <step type="toiso">
        <!-- Conversion xml unimarc en iso2709 unimarc -->
        <rootelement>unimarc</rootelement>
        <il>2</il>
        <sl>2</sl>
        <dm1>4</dm1>
        <dm2>5</dm2>
        <dm3>0</dm3>
    </step>
</params>