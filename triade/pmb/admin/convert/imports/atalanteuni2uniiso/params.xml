<params>
    <!-- Nom de l'import -->
    <importname>UNIMARC Atalante -&gt; UNIMARC ISO 2709</importname>
    <!-- Nombre de notices traitées par passe -->
    <nperpass>200</nperpass>
    <!-- Type de l'entrée -->
    <input type="custom" script="atalandeuni2uniiso_input.class.php"/>
    <!-- type de la sortie -->
	<output importable="yes" type="iso_2709" suffix="marc"/>

    <step type="custom">
        <!-- Conversion texte atalante en unimarc -->
        <script>atalanteuni2uniiso.class.php</script>
		<callback>convert_data</callback>
    </step>
	<step type="toiso">
		<rootelement>*</rootelement>
		<il>2</il>
		<sl>2</sl>
		<dm1>4</dm1>
		<dm2>5</dm2>
		<dm3>0</dm3>
	</step>
</params>