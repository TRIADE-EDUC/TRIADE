	<content lang='en_UK'><![CDATA[
		<h2>Database</h2>
		<div class='row'>
			<div class='title'>Records : </div>
			<div class='value'>{{nb_records.0.total}}</div>
		</div>
		<div class='row'>
			<div class='title'>of which periodical titles : </div>
			<div class='value'>{{nb_records_perio.0.total}}</div>
		</div>
		<div class='row'>
			<div class='title'>of which articles : </div>
			<div class='value'>{{nb_records_articles.0.total}}</div>
