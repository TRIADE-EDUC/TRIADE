			<div class='title'>To renew : </div>
			<div class='value'>{{nb_abon_renouv.0.total}}</div>
		</div>
		<div class='row'>
			<div class='title'>Overdue : </div>
			<div class='value'>{{nb_abon_depass.0.total}}</div>
		</div>
		
