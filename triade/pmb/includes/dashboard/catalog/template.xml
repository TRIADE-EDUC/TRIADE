		<div class='row'>
			<div class='title'>A renouveler : </div>
			<div class='value'>{{nb_abon_renouv.0.total}}</div>
		</div>
		<div class='row'>
			<div class='title'>Dépassés : </div>
			<div class='value'>{{nb_abon_depass.0.total}}</div>
		</div>
		
		]]>
