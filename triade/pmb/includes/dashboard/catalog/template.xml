		<div class='row'>
			<div class='title'>To be delivered : </div>
			<div class='value'>{{nb_records_recevoir.0.total}}</div>
		</div>
		<div class='row'>
			<div class='title'>Next issues : </div>
			<div class='value'>{{nb_records_prochain.0.total}}</div>
		</div>
