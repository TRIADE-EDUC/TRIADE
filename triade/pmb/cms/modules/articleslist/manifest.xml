<manifest>
	<name lang="fr_FR">Liste d'articles</name>
	<name lang="en_UK">List of articles</name>
	<author>
		<name>Dimitri GORON</name>
		<organisation>PMB Services</organisation>
	</author>
	<created_date>2012-05-16</created_date>
	<version>1.0</version>
	<default_language>fr_FR</default_language>
	<managed>true</managed>
	<use>
		<datasources>
			<datasource>cms_module_common_datasource_articles_sections</datasource>
			<datasource>cms_module_common_datasource_all_articles</datasource>
			<datasource>cms_module_common_datasource_articles</datasource>
			<datasource>cms_module_common_datasource_articles_categories</datasource>
			<datasource>cms_module_common_datasource_searcharticles</datasource>
			<datasource>cms_module_common_datasource_articles_by_article_categories</datasource>
			<datasource>cms_module_common_datasource_articles_by_section_categories</datasource>
			<datasource>cms_module_common_datasource_articles_by_categories</datasource>
			<datasource>cms_module_common_datasource_articles_by_section_cp</datasource>
			<datasource conditionParam="thesaurus_concepts_active">cms_module_common_datasource_articles_by_concepts</datasource>
		</datasources>
		<filters>
			<filter>cms_module_common_filter_articles_by_all_cp</filter>
			<filter>cms_module_common_filter_articles_by_cp</filter>
			<filter>cms_module_common_filter_articles_by_type</filter>
			<filter>cms_module_common_filter_articles_by_publication_state</filter>
		</filters>
		<views>
			<view>cms_module_common_view_articleslist</view>
			<view>cms_module_articleslist_view_carousel</view>
			<view>cms_module_articleslist_view_carousel_responsive</view>
		</views>
		<conditions>
			<condition>cms_module_common_condition_global_var</condition>
		</conditions>
	</use>
</manifest>